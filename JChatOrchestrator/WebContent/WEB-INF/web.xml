		<servlet-name>com.rombachuk.jchatorchestrator.HistoricEventsServlet</servlet-name>
	<servlet-class>com.rombachuk.jchatorchestrator.HistoricEventsServlet</servlet-class>
	</servlet>
	<servlet>
		<display-name>CustomerServiceServlet</display-name>
		<servlet-name>com.rombachuk.jchatorchestrator.CustomerServiceServlet</servlet-name>
	<servlet-class>com.rombachuk.jchatorchestrator.CustomerServiceServlet</servlet-class>
	</servlet>
	<servlet>
		<display-name>chatstartServlet</display-name>
		<servlet-name>
			com.rombachuk.jchatorchestrator.chatstartServlet
	</servlet-name>
	<servlet-class>com.rombachuk.jchatorchestrator.chatstartServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>
			com.rombachuk.jchatorchestrator.chatstartServlet
		</servlet-name>
		<url-pattern>/chatstart</url-pattern>
	</servlet-mapping>
		<servlet>
		<display-name>showstartServlet</display-name>
		<servlet-name>
			com.rombachuk.jchatorchestrator.showstartServlet
	</servlet-name>
	<servlet-class>com.rombachuk.jchatorchestrator.showstartServlet</servlet-class>
