			com.rombachuk.jchatorchestrator.LaunchSessionCheckServlet
		</servlet-name>
		<url-pattern>/launchsessioncheck</url-pattern>
	</servlet-mapping>
	<context-param>
		<param-name>jcoProperties</param-name>
		<param-value>/WEB-INF/jco.props</param-value>
	</context-param>
	<context-param>
		<param-name>jcoWorkspaces</param-name>
		<param-value>/WEB-INF/jco_workspaces.props</param-value>
	</context-param>
	<servlet-mapping>
		<servlet-name>com.rombachuk.jchatorchestrator.EventAnalyticsServlet</servlet-name>
		<url-pattern>/chat/eventanalytics</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com.rombachuk.jchatorchestrator.CustomerServiceServlet</servlet-name>
		<url-pattern>/chat/customerservice</url-pattern>
	</servlet-mapping>
	<context-param>
		<param-name>eventbotObjectserverFields</param-name>
		<param-value>/WEB-INF/eventbot_objectserver_fields.def</param-value>
	</context-param>
	<context-param>
		<param-name>eventbotProperties</param-name>
		<param-value>/WEB-INF/eventbot.props</param-value>
	</context-param>
		<servlet-mapping>
		<servlet-name>com.rombachuk.jchatorchestrator.RelatedEventsServlet</servlet-name>
		<url-pattern>/show/relatedevents</url-pattern>
	</servlet-mapping> 
		<servlet-mapping>
		<servlet-name>com.rombachuk.jchatorchestrator.SeasonalEventsServlet</servlet-name>
		<url-pattern>/show/seasonalevents</url-pattern>
	</servlet-mapping> 
			<servlet-mapping>
		<servlet-name>com.rombachuk.jchatorchestrator.HistoricEventsServlet</servlet-name>
		<url-pattern>/show/historicevents</url-pattern>
	</servlet-mapping>
		<context-param>
			<param-name>jcoDialogueAssistants</param-name>
			<param-value>/WEB-INF/jco_dialogue_assistants.props</param-value>
		</context-param>
</web-app>
