<?xml version="1.0" encoding="UTF-8"?>
<web-app id="JChatOrchestrator" version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
	<display-name>JChatOrchestrator</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
		<filter>
		<filter-name>ChatFilter</filter-name>
		<filter-class>
			com.rombachuk.jchatorchestrator.ChatFilter
		</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>ChatFilter</filter-name>
		<url-pattern>/chat/*</url-pattern>
		<dispatcher>REQUEST</dispatcher></filter-mapping>
	    <filter>
		<filter-name>ShowFilter</filter-name>
		<filter-class>
			com.rombachuk.jchatorchestrator.ShowFilter
		</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>ShowFilter</filter-name>
		<url-pattern>/show/*</url-pattern>
		<dispatcher>REQUEST</dispatcher></filter-mapping>
	<listener>
	<description>SessionListener</description>
	<listener-class>
		com.rombachuk.jchatorchestrator.SessionListener
	</listener-class>
</listener>
<listener>  
<description>JcoContextListener</description>
    <listener-class>
        com.rombachuk.jchatorchestrator.JcoContextListener
    </listener-class>
</listener>  
	<servlet>
		<display-name>loginServlet</display-name>
	<servlet-name>
			com.rombachuk.jchatorchestrator.loginServlet
	</servlet-name>
	<servlet-class>com.rombachuk.jchatorchestrator.loginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>
			com.rombachuk.jchatorchestrator.loginServlet
		</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
		<servlet>
		<display-name>loginServlet</display-name>
	<servlet-name>
			com.rombachuk.jchatorchestrator.logoutServlet
	</servlet-name>
	<servlet-class>com.rombachuk.jchatorchestrator.logoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>
			com.rombachuk.jchatorchestrator.logoutServlet
		</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>EventAnalyticsServlet</display-name>
		<servlet-name>com.rombachuk.jchatorchestrator.EventAnalyticsServlet</servlet-name>
	<servlet-class>com.rombachuk.jchatorchestrator.EventAnalyticsServlet</servlet-class>
	</servlet>
		<servlet>
